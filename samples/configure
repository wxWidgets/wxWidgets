    echo "cc -dynamiclib master.\$\$.o \$args"
fi
c++ -dynamiclib master.\$\$.o \$args
status=\$?
if test \$status != 0; then
    exit \$status
fi

#
# Remove intermediate module
#
rm -f master.$$.o

exit 0
EOF >shared-ld-sh
