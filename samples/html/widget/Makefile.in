data: 
	@mkdir -p .
	@for f in start.htm; do \
	if test \( ! -s ./$$f \) -o \( $(srcdir)/$$f -nt ./$$f  \) ; then \
	cp -pRf $(srcdir)/$$f . ; \
	fi; \
	done

