                        </object>
                        <vgap>5</vgap>
                        <hgap>5</hgap>
                      </object>
                      <flag>wxALL</flag>
                      <border>5</border>
                    </object>
                    <label>Calculator</label>
                    <orient>wxVERTICAL</orient>
                  </object>
                  <flag>wxTOP|wxALIGN_CENTRE_HORIZONTAL</flag>
                  <border>10</border>
                </object>
                <object class="sizeritem">
                  <object_ref ref="bottom_panel"/>
                  <flag>wxEXPAND</flag>
                </object>
                <cols>1</cols>
                <rows>0</rows>
                <vgap>0</vgap>
