        <border>20</border>
      </object>
      <cols>1</cols>
      <rows>0</rows>
      <vgap>0</vgap>
      <hgap>0</hgap>
      <growablecols>0</growablecols>
      <growablerows>0</growablerows>
    </object>
    <title>Object References and ID Ranges Example</title>
    <centered>1</centered>
    <style>wxCAPTION|wxSYSTEM_MENU|wxRESIZE_BORDER</style>
    <exstyle>wxDIALOG_EX_CONTEXTHELP</exstyle>
  </object>
