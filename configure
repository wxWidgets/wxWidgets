s%@COMMONDEPS@%$COMMONDEPS%g
s%@GENERICDEPS@%$GENERICDEPS%g
s%@IODBCOBJS@%$IODBCOBJS%g
s%@UNIXOBJS@%$UNIXOBJS%g
s%@UNIXDEPS@%$UNIXDEPS%g
s%@ALL_OBJECTS@%$ALL_OBJECTS%g
s%@ALL_DEPFILES@%$ALL_DEPFILES%g
s%@GUIDIST@%$GUIDIST%g
s%@PORT_FILES@%$PORT_FILES%g
