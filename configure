  echo "$CHECK_INCLUDE" | grep "\-I$ac_find_includes" > /dev/null
  result=$?
  if test $result = 0; then
    ac_path_to_include=""
  else
    ac_path_to_include="-I$ac_find_includes"    
  fi

        
  echo "$CHECK_LIB" | grep "\-L$ac_find_libraries" > /dev/null
