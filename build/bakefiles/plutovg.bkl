<?xml version="1.0" ?>
<makefile>

    <if cond="FORMAT=='autoconf'">
        <option name="wxUSE_LUNASVG"/>
        <set var="LIB_PLUTOVG">
            <if cond="USE_LUNASVG=='1'">
                wxplutovg$(WXDEBUGFLAG)$(WX_LIB_FLAVOUR)-$(WX_RELEASE)$(HOST_SUFFIX)
            </if>
        </set>
    </if>
    <if cond="FORMAT!='autoconf'">
        <set var="LIB_PLUTOVG">
            <if cond="USE_LUNASVG=='1'">
                wxplutovg$(WXDEBUGFLAG)$(HOST_SUFFIX)
            </if>
        </set>
    </if>
    <set var="INC_PLUTOVG">
        <if cond="USE_LUNASVG=='1'">$(TOP_SRCDIR)3rdparty/lunasvg/3rdparty/plutovg</if>
    </set>

    <lib id="wxplutovg" template="3rdparty_lib"
         cond="USE_LUNASVG=='1' and BUILDING_LIB=='1'">
        <dirname>$(LIBDIRNAME)</dirname>
        <if cond="FORMAT!='autoconf'">
            <include>$(TOP_SRCDIR)3rdparty/lunasvg/3rdparty/plutovg</include>
        </if>
        <sources>
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-blend.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-dash.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-ft-math.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-ft-raster.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-ft-stroker.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-geometry.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-paint.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg-rle.c
            3rdparty/lunasvg/3rdparty/plutovg/plutovg.c
        </sources>
    </lib>

</makefile>
