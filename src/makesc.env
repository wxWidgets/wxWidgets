# Common settings for Symantec

WXDIR = $(WXWIN)
INCDIR = $(WXDIR)\include
MSWINC = $(INCDIR)\msw
BASEINC = $(INCDIR)\base

SRCSUFF = cpp
OBJSUFF = obj

# default values

CC=sc
RC=rc
CFLAGS = -o -ml -W -D__WINDOWS__
LDFLAGS = -ml -W

.$(SRCSUFF).obj:
	*$(CC) -c $(CFLAGS) -I$(INCLUDE) $(OPTIONS) $<

